{
  "manifest_version": 3,
  "name": "Moodle Editor Switcher",
  "description": "Quickly switch between Moodle text editors",
  "version": "1.0",
  "permissions": ["storage"],
  "host_permissions": ["https://moodle.hftm.ch/*"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
